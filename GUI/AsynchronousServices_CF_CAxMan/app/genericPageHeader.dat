  function soapMsg() {
            var xmlreqhttp = new XMLHttpRequest();
            xmlreqhttp.open('POST', 'https://api.caxman.eu/igd/examples/WebApplication', true);
			var xmlOut = getJSONObject();
			//alert(b64_xmlOut);
            //Soap Request
            var sr ='<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:sam="http://example.com/sample.wsdl">'+
					'<soapenv:Header/>'+
					'<soapenv:Body>'+
					'<sam:serviceExecutionFinishCall>'+
						'<sam:xmlOutputs>'+xmlOut+'</sam:xmlOutputs>'+
					'</sam:serviceExecutionFinishCall>'+
					'</soapenv:Body>'+
					'</soapenv:Envelope>';    
            xmlreqhttp.onreadystatechange = function () {
                if (xmlreqhttp.readyState == 4) {
				 //alert('ready');
                    if (xmlreqhttp.status == 200) {
                      // alert('Done');
                    }
					else
					{	
						alert('Error while Sending Request status: '+xmlreqhttp.status+' Response :'+xmlreqhttp.responseText);
					}
                }
            }
            //For sending the Post Request
           //xmlreqhttp.setRequestHeader('Content-Type', 'text/xml');
           xmlreqhttp.send(sr);
		}
		
	//Validation
	function validatefields()
	{
	  
		soapMsg();
	}
	
	
	
